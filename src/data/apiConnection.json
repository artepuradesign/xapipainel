  "database_schema": {
    "mysql_tables": {
      "users": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "username": "VARCHAR(50) UNIQUE NOT NULL",
        "email": "VARCHAR(100) UNIQUE NOT NULL",
        "password_hash": "VARCHAR(255) NOT NULL",
        "full_name": "VARCHAR(100) NOT NULL",
        "plan_id": "INT",
        "wallet_balance": "DECIMAL(10,2) DEFAULT 0.00",
        "plan_balance": "DECIMAL(10,2) DEFAULT 0.00",
        "status": "ENUM('active', 'inactive', 'suspended') DEFAULT 'active'",
        "user_role": "ENUM('assinante', 'suporte', 'admin') DEFAULT 'assinante'",
        "last_login": "TIMESTAMP NULL",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP",
        "updated_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP"
      },
      "plans": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "name": "VARCHAR(50) NOT NULL",
        "card_suit": "VARCHAR(10)",
        "card_type": "VARCHAR(10)",
        "discount_percentage": "INT DEFAULT 0",
        "monthly_price": "DECIMAL(10,2)",
        "query_limit": "INT DEFAULT -1",
        "features": "JSON",
        "is_active": "BOOLEAN DEFAULT TRUE",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP"
      },
      "consultation_modules": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "name": "VARCHAR(50) NOT NULL",
        "description": "TEXT",
        "icon": "VARCHAR(50)",
        "path": "VARCHAR(100)",
        "basic_price": "DECIMAL(10,2)",
        "complete_price": "DECIMAL(10,2)",
        "premium_price": "DECIMAL(10,2)",
        "is_active": "BOOLEAN DEFAULT TRUE",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP"
      },
      "consultations": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT",
        "module_id": "INT",
        "document": "VARCHAR(20) NOT NULL",
        "consultation_type": "ENUM('basic', 'complete', 'premium')",
        "original_price": "DECIMAL(10,2)",
        "final_price": "DECIMAL(10,2)",
        "discount_applied": "INT",
        "status": "ENUM('success', 'error', 'pending')",
        "response_data": "JSON",
        "ip_address": "VARCHAR(45)",
        "user_agent": "TEXT",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP",
        "FOREIGN KEY (user_id)": "REFERENCES users(id)",
        "FOREIGN KEY (module_id)": "REFERENCES consultation_modules(id)"
      },
      "balance_transactions": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT",
        "transaction_type": "ENUM('deposit', 'withdrawal', 'consultation', 'commission', 'bonus')",
        "amount": "DECIMAL(10,2)",
        "balance_type": "ENUM('wallet', 'plan') DEFAULT 'wallet'",
        "description": "VARCHAR(255)",
        "consultation_id": "INT NULL",
        "reference_id": "VARCHAR(100) NULL",
        "previous_balance": "DECIMAL(10,2)",
        "new_balance": "DECIMAL(10,2)",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP",
        "FOREIGN KEY (user_id)": "REFERENCES users(id)",
        "FOREIGN KEY (consultation_id)": "REFERENCES consultations(id)"
      },
      "payment_methods": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT",
        "type": "ENUM('pix', 'credit_card', 'bank_transfer', 'paypal', 'crypto')",
        "provider": "VARCHAR(50)",
        "details": "JSON",
        "is_default": "BOOLEAN DEFAULT FALSE",
        "is_active": "BOOLEAN DEFAULT TRUE",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP",
        "FOREIGN KEY (user_id)": "REFERENCES users(id)"
      },
      "api_keys": {
        "id": "INT AUTO_INCREMENT PRIMARY KEY",
        "user_id": "INT",
        "key_hash": "VARCHAR(255) NOT NULL",
        "name": "VARCHAR(100)",
        "permissions": "JSON",
        "rate_limit": "INT DEFAULT 100",
        "is_active": "BOOLEAN DEFAULT TRUE",
        "last_used": "TIMESTAMP NULL",
        "expires_at": "TIMESTAMP NULL",
        "created_at": "TIMESTAMP DEFAULT CURRENT_TIMESTAMP",
        "FOREIGN KEY (user_id)": "REFERENCES users(id)"
      }
    }
  },
